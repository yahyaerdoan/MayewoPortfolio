@model List<MayewoPortfolio.Models.Aboute>
@foreach (var item in Model)
{
    <section class="section pt-0" id="about">
        <!-- container -->
        <div class="container text-center">
            <!-- about wrapper -->
            <div class="about">
                <div class="about-img-holder">
                    <img src="~/Templates/meyawo-1.0.0/assets/imgs/algoritmaq.jpg" style="border-radius:35px" class="about-img"
                         alt="Download free bootstrap 4 landing page, free boootstrap 4 templates, Download free bootstrap 4.1 landing page, free boootstrap 4.1.1 templates, meyawo Landing page">
                </div>
                <div class="about-caption">
                    <p class="section-title"><b>@item.Title</b></p>
                    <h2 class="section-title mb-3">@item.Header</h2>
                    <p><b>@item.Description</b></p>

                    <a href="~/Templates/meyawo-1.0.0/ProjectDocument/YahyaERDOGANResume.pdf" download="CV" class="btn-rounded btn btn-outline-primary mt-4">Download My Resume</a>
                </div>
            </div><!-- end of about wrapper -->
        </div><!-- end of container -->
    </section>
}
